class Tables
{
    public static void main(String[] args)
    {
        int number=20;
        for(int i=1;i<=number;i++)
        {
            System.out.println("Multiplication for:"+i);
            for(int j=1;j<=10;j++)
            {
                int product=i*j;
                System.out.println(i + "*" + j + "=" + product);
            }
            System.out.println();
        }
    }
}
