public class Max{
    public static void main(String[] args){
        int[] arr={23,28,10,29,20};
        System.out.println(max(arr));
    }
    static int max(int[] arr) {
        int maxVal=arr[3];
        for(int i=1;i<arr.length;i++){
            if(arr[i]>maxVal){
                maxVal=arr[i];
            }
        }
        return maxVal;
    }
}    
