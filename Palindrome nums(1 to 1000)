class Main
{
    public static void main(String[] args)
    {
        for(int i=1;i<=1000;i++)
        {
            int num=i;
            int ans=0;
            while(num>0)
            {
               int rem=num%10;
                num=num/10;
                ans=ans*10+rem;
            }
            if(ans==i)
            {
                System.out.print(i+" ");
            }
        }
    }
}
